@model IEnumerable<Commodity.Models.ImportOrder>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ImportOrderName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImportDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ImportOrderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImportDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ImportOrderID"><i class="fas fa-edit"></i></a> |
                <a asp-action="Details" asp-route-id="@item.ImportOrderID"><i class="fas fa-eye"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.ImportOrderID"><i class="fas fa-trash-alt"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
